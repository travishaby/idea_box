<div class="row">
  <div class="col-md-6">
    <h3>Please Create An Account</h1>

    <%= form_for(@user) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <%= f.submit "Create Account"%>
    <% end %>
  </div>
  <div class="col-md-6" id="sign_in">

  </div>
</div>
